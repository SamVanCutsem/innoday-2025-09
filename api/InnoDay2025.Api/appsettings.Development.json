{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Information"
      }
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=innoday2025-dev.db"
  },
  "ApiSettings": {
    "Name": "InnoDay 2025 API - Development",
    "Version": "1.0.0-dev",
    "Description": "Development REST API for InnoDay 2025 project",
    "ContactEmail": "dev-team@innoday2025.com"
  },
  "CorsSettings": {
    "AllowedOrigins": [
      "http://localhost:3000",
      "https://localhost:3000",
      "http://localhost:3001",
      "https://localhost:3001"
    ]
  },
  "RateLimiting": {
    "EnableRateLimiting": false,
    "RequestsPerMinute": 1000
  }
}