{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=innoday2025.db"
  },
  "ApiSettings": {
    "Name": "InnoDay 2025 API",
    "Version": "1.0.0",
    "Description": "REST API for InnoDay 2025 project",
    "ContactEmail": "team@innoday2025.com"
  },
  "CorsSettings": {
    "AllowedOrigins": [
      "http://localhost:3000",
      "https://localhost:3000"
    ]
  },
  "HealthChecks": {
    "UI": {
      "EvaluationTimeInSeconds": 10,
      "MinimumSecondsBetweenFailureNotifications": 60
    }
  },
  "RateLimiting": {
    "EnableRateLimiting": true,
    "RequestsPerMinute": 100
  },
  "AllowedHosts": "*"
}